{
  "name": "vpia",
  "version": "0.1.0",
  "description": "Virtual Platform for Indigenous Art",
  "main": "./index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./MainContainer/config/webpack.config.prod.js --progress --profile --colors",
    "dev": "node index.js",
    "prep": "cd MediaService && npm i",
    "microservices": "cd MediaService && npm run start",
    "start": "npm run build && cross-env NODE_ENV=production node index.js && SET AUTOUPDATE=1 & npm run microservices",
    "prettier": "prettier --write \"**/*.{js,jsx}\" --ignore-path .gitignore",
    "lint": "eslint --config ./MainContainer/config/.eslintrc.js \"**/*.js\" --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": "~0.12.0 || ^4.2.0 || ^6.5.0"
  },
  "author": "M. L. Filipp <contact@mordax.io> (https://mordax.io)",
  "contributors": [
    "Y. Shao <yolandayiyishao@gmail.com> (https://yiyishao.org/)"
  ],
  "license": "(GPL-3.0 OR MIT)",
  "dependencies": {
    "@tinymce/tinymce-react": "^3.3.2",
    "bcryptjs": "2.4.0",
    "body-parser": "^1.19.0",
    "caminte": "^0.4.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.0.0",
    "mysql": "^2.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "3.2.0",
    "react-router-dom": "^5.1.2",
    "react-status-alert": "^1.0.0",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "file-loader": "^4.3.0",
    "prettier": "1.18.2",
    "react-hot-loader": "3.0.0-beta.6",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.1.0",
    "cross-env": "3.1.4",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "compression": "^1.7.4",
    "postcss-loader": "^3.0.0",
    "webpack": "4.42.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.0.1"
  }
